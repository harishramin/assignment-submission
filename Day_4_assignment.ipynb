        "# Day 4 assignment\n",
        "n = int(input(\"Enter number here : \"))\n",
        "def factorial_iter(n):\n",
        "    product = 1\n",
        "    for i in range(n):\n",
        "        product = product * (i+1)\n",
        "    return product\n",
        "\n",
        "f = factorial_iter(n)\n",
        "print(f)"
